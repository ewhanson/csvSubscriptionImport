msgid "plugins.importexport.subscriptionImporter.displayName"
msgstr "Subscription Importer Plugin"

msgid "plugins.importexport.subscriptionImporter.description"
msgstr "Import user subscription details from comma delimited data."

msgid "plugins.importexport.subscriptionImporter.cliOnly"
msgstr ""
"\n"
"\t\t<p>This plugin currently supports command-line operation only. Execute...\n"
"\t\t\t<pre>php tools/importExport.php CSVImportExportPlugin</pre>\n"
"\t\t\t...for more information.</p>\n"
"\t"

msgid "plugins.importexport.subscriptionImporter.cliUsage"
msgstr ""
"Command-line tool for updating user subscription information in OJS\n"
"\t\t\tUsage:\n"
"\t\t\t{$scriptName} fileName.csv [journal_path] [subscription_type_id] outputFile.csv [--test]\n"
"\t\t\tThe --test option can be used to append 'test' at the end of user emails.\n"
""

msgid "plugins.importexport.subscriptionImporter.fileDoesNotExist"
msgstr "The file \"{$filename}\" does not exist.  Exiting."
